<!DOCTYPE html>
<html>
<head>
    <title>Administration | <%= @page_title || "Admin" %></title>
    <%= stylesheet_link_tag('application', :media => 'all') %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= javascript_pack_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
<body>
<%= render 'sidebar'%>
<%= render 'topbar' %>


  <div>
    <div class="container-fluid main">
      <%= render('breadcrumb') %>
      <!-- main content div -->
      <div id="flash-div"><%= render('flash') %></div>
      <%= render('content_container') %> <!-- yield is within this partial -->
      <%= render('footer') %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </div>
</body>
</html>
