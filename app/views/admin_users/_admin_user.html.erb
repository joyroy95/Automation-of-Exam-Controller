<div class="row">
	<div class="col-xs-6 col-sm-2">
		<p><%= admin_user.name %></p>
	</div>
	<div class="col-xs-6 col-sm-2">
		<p><%= admin_user.username %></p>
	</div>
	<div class="col-xs-6 col-sm-2">
		<p><%= admin_user.email %></p>
	</div>
	<div class="col-xs-6 col-sm-3">
			<%= show_button admin_user %>
			<%= edit_button admin_user %>
			<%= delete_button admin_user, :data => {confirm: 'Are you sure you want to delete this user?'} %>
	</div>
</div>
